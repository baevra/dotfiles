  "tokenColors": [
    {
      "scope": [
        "keyword.operator.arithmetic.js",
        "meta.brace.square.js",
        "punctuation.separator.comma.js",
        "punctuation.accessor.js",
        "meta.brace.round.js",
        "meta.type.object.graphql",
        "meta.brace.round.graphql",
        "variable.parameter.graphql",
        "punctuation.operation.graphql",
        "punctuation.colon.graphql",
        "source",
        "variable.other.readwrite",
        "meta.embedded.line.js variable.other.object.js",
        "source.js meta.objectliteral.js meta.object.member.js meta.object-literal.key.js entity.name.function.js",
        // "string.unquoted.plain.out.yaml"
      ],
      "settings": { "foreground": "#ffffff" }
    },
    {
      "name": "Comments",
      "scope": [
        "comment", // //
        "comment storage.type" // /** @class */
      ],
      "settings": { "foreground": "#7F8C97", "fontStyle": "italic" }
    },
    {
      "name": "Strings",
      "scope": [
        "source.js punctuation.definition.string.template",
        "string.quoted.double.js",
        "string",
        "variable.parameter.url.scss",
        "markup.heading.markdown",
        "beginning.punctuation.definition.list.markdown",
        "meta.template.expression meta.embedded punctuation.definition.string.begin",
        "meta.template.expression meta.embedded punctuation.definition.string.end"
      ],
      "settings": { "foreground": "#FD8273" }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric.env",
        "source.yaml constant.numeric",
        "source.css support.constant",
        "meta.property-value.css",
        "constant.numeric.css",
        "source.css keyword.other.unit",
        "source.css support.constant.color.",
        "constant.numeric.integer.yaml",
        "constant.numeric.json",
        "constant.numeric.json.comments",
        "constant.numeric.decimal.js"
      ],
      "settings": { "foreground": "#A79FF5" }
    },
    {
      "name": "Keywords",
      "scope": [
        "constant.language.undefined.js",
        "keyword.other.special-method.dockerfile",
        "constant.language.null.yaml",
        "constant.language.boolean.yaml",
        "source.js keyword.operator.expression",
        "source.js constant.language.boolean",
        "variable.language.super.js",
        "constant.language.null.js",
        "storage.type.class.js",
        "storage.modifier.js",
        "variable.language.this.js",
        "storage.type.function.js",
        "constant.language.json",
        "constant.language.boolean.false.js",
        "constant.language.json.comments",
        "storage.modifier.async.js",
        "keyword.control",
        "keyword.operator.new.js",
        "storage.type.js",
        "keyword.operator.new.js",
        "constant.other.boolean",
      ],
      "settings": { "foreground": "#FD7CB2", "fontStyle": "bold" }
    },
    {
      "name": "Keywords light",
      "scope": [
        "keyword.fragment.graphql",
        "punctuation.definition.table.toml",
        "entity.other.attribute-name.table.toml",
        "meta.tag.table.toml",
        "keyword.input.graphql",
        "entity.name.function.graphql",
        "entity.name.function.directive.graphql",
        "entity.name.function.directive.graphql",
        "keyword.operation.graphql",
        "source.graphql meta.type.interface.graphql keyword.type.graphql",
        "keyword.type.graphql",
        "keyword.enum.graphql"
      ],
      "settings": { "foreground": "#FD7CB2" }
    },
    {
      "name": "Project Instance Variables and Globals",
      "scope": [
        "entity.name.fragment.graphql",
        "support.type.enum.graphql",
        "support.type.graphql",
        "support.type.builtin.graphql",
        "variable.other.property.js",
        "variable.other.object.property.js",
        "support.variable.property",
        "variable.other.constant.property",
        "entity.name.type.js",
        "source.js meta.arrow.js meta.block.js variable.other.property.js",
        "source.js meta.arrow.js meta.block.js meta.function-call.js variable.other.object.property.js"
      ],
      "settings": { "foreground": "#A1D87A" }
    },
    {
      "name": "Project Function and Method Names",
      "scope": [
        "support.function",
        "support.function.json.js",
        "entity.name.function.ts",
        "support.function.console.js",
        "entity.name.function.js",
        "source.js meta.arrow.js meta.block.js meta.function-call.js support.function.console.js",
        
      ],
      "settings": { "foreground": "#BBF994" }
    },
    {
      "name": "Other Instance Variables and Globals",
      "scope": [
        "keyword.control.anchor.regexp",
        "string.regexp.js",
        "support.constant",
        "support.constant.json.js",
        "source.js support.class",
        "constant.character.enum.graphql",
        "entity.name.tag",
        "source.css entity.other.attribute-name",
        "entity.other.inherited-class.js",
        "entity.name.type.class.js",
        "variable.graphql",
        "support.type.vendored.property-name.css",
        "entity.other.attribute-name.js",
        "support.type.property-name.css",
        "support.variable.object",
        "entity.name.tag.yaml",
        "punctuation.definition.template-expression.begin.js",
        "punctuation.definition.template-expression.end.js",
        "support.class.builtin.js",
        "support.type.property-name.json",
        "support.type.property-name.json.comments",
        "source.js support.type.object.module.js",
        "source.js meta.var.expr.js meta.function-call.js support.function.js",
        "source.js meta.function.js meta.block.js meta.var.expr.js new.expr.js entity.name.type.js"
      ],
      "settings": { "foreground": "#8CD1C3"}
    }
  ]
}